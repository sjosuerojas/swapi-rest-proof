{
    "name": "swapi-client",
    "version": "1.1.0",
    "private": true,
    "scripts": {
        "start": "webpack serve --config ./webpack/dev.config.js --hot",
        "build:dll": "webpack --config ./webpack/dev.dll.config.js --mode production",
        "build:prod": "webpack --config ./webpack/prod.config.js --mode production",
        "build": "npm run build:dll && npm run build:prod",
        "format": "prettier --write '{*.js,src/**/*.{js,jsx}}'",
        "lint": "eslint src/ --fix"
    },
    "keywords": [
        "react",
        "react-dom",
        "babel",
        "eslint",
        "prettier",
        "webpack",
        "node"
    ],
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "formik": "^2.2.9",
        "react": "^17.0.2",
        "react-burger-menu": "^3.0.6",
        "react-dom": "^17.0.2",
        "react-lazy-load-image-component": "^1.5.1",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "redux": "^4.1.0",
        "redux-thunk": "^2.3.0",
        "sweetalert2": "^11.0.17",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-react": "^7.12.7",
        "@babel/runtime": "^7.12.5",
        "add-asset-html-webpack-plugin": "^3.1.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^5.0.1",
        "css-minimizer-webpack-plugin": "^1.1.5",
        "dotenv-webpack": "^6.0.0",
        "eslint": "^7.14.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.2.0",
        "eslint-plugin-react": "^7.21.5",
        "file-loader": "^6.2.0",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^1.3.1",
        "node-sass": "^5.0.0",
        "prettier": "^2.2.1",
        "sass-loader": "^10.1.0",
        "terser-webpack-plugin": "^5.0.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.9.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    }
}
