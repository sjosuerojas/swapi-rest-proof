{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|docs)",
      "jsdoc": "package.json"
    },
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "template": "templates/default", 
      "encoding": "utf8",               
      "destination": "./docs/",          
      "recurse": true                 
  }
  }
  